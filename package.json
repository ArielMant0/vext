{
  "name": "@nullbuild/vext",
  "version": "0.2.2",
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/vext.umd.js",
  "module": "./dist/vext.cjs",
  "style": "./dist/style.css",
  "exports": {
    ".": {
      "import": "./dist/vext.js",
      "require": "./dist/vext.umd.cjs"
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ArielMant0/vext.git"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint . --fix --ignore-path .gitignore",
    "docs:gen": "vue-docgen src/components/**/*.vue docs",
    "docs:dev": "concurrently \"vue-docgen src/components/**/*.vue docs -w\" \"vitepress dev docs\"",
    "docs:build": "vue-docgen src/components/**/*.vue docs && vitepress build docs",
    "docs:build-vite": "vitepress build docs",
    "docs:preview": "vitepress preview docs"
  },
  "dependencies": {
    "@mdi/font": "7.0.96",
    "@vueuse/core": "^9.13.0",
    "core-js": "^3.8.3",
    "fabric": "^5.3.0",
    "file-saver": "^2.0.5",
    "html-to-image": "^1.11.11",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "object-hash": "^3.0.0",
    "pinia": "^2.0.23",
    "roboto-fontface": "*",
    "uuid": "^9.0.0",
    "vue": "^3.2.13",
    "vue-json-pretty": "^2.2.3",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0",
    "webfontloader": "^1.0.0"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^3.0.3",
    "d3": "^7.8.4",
    "eslint": "^8.22.0",
    "eslint-plugin-vue": "^9.3.0",
    "sass": "^1.55.0",
    "vite": "^3.1.9",
    "vite-plugin-libcss": "^1.0.6",
    "vite-plugin-vuetify": "^1.0.0-alpha.12",
    "vitepress": "^1.0.0-alpha.49",
    "vue-docgen-cli": "^4.60.1"
  },
  "peerDependencies": {
    "@vueuse/core": "^9.13.0",
    "fabric": "^5.3.0",
    "file-saver": "^2.0.5",
    "html2canvas": "^1.4.1",
    "jspdf": "^2.5.1",
    "jszip": "^3.10.1",
    "object-hash": "^3.0.0",
    "pinia": "^2.0.23",
    "uuid": "^9.0.0",
    "vue": "^3.2.13",
    "vue-json-pretty": "^2.2.3",
    "vue-router": "^4.0.0",
    "vuetify": "^3.0.0"
  }
}
